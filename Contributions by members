import pandas as pd
import matplotlib.pyplot as plt
df = pd.read_csv('fairfield.csv',encoding='latin-1', usecols=['Mercury','Fish','Elevation'])
print(df.describe())
plt.hist(df['Mercury'])



df = pd.read_csv('fairfield.csv',encoding='latin-1', usecols=['Fish'])
print(df.describe())
